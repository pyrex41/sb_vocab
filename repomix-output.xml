var current_session = null
var session_progress = []

func start_session(grade_level: int = 3) -> Dictionary:
	var session_words = vocabulary_data.filter(func(word): return word.grade == grade_level)
	if session_words.size() > 10:
		session_words.shuffle()
		session_words = session_words.slice(0, 10)
	
	current_session = {
		"session_id": Time.get_unix_time_from_system(),
		"words": session_words,
		"activities": _generate_activities(session_words),
		"current_index": 0,
		"score": 0,
		"total_attempts": 0
	}
	session_progress = []
	return current_session

func _generate_activities(words: Array) -> Array:
	var activities = []
	var activity_types = ["flashcard", "multiple_choice", "spelling", "fill_blank", "synonym_antonym"]
	
	for word in words:
		for i in range(2):
			var activity_type = activity_types[randi() % activity_types.size()]
			activities.append({
				"type": activity_type,
				"word": word
			})
	
	activities.shuffle()
	return activities

func get_current_activity() -> Dictionary:
	if current_session == null or current_session.current_index >= current_session.activities.size():
		return {}
	return current_session.activities[current_session.current_index]

func submit_attempt(answer: String, is_correct: bool) -> Dictionary:
	if current_session == null:
		return {"success": false, "message": "No active session"}
	
	var activity = get_current_activity()
	current_session.total_attempts += 1
	if is_correct:
		current_session.score += 1
	
	session_progress.append({
		"activity": activity,
		"answer": answer,
		"correct": is_correct,
		"timestamp": Time.get_unix_time_from_system()
	})
	
	current_session.current_index += 1
	
	return {
		"success": true,
		"correct": is_correct,
		"score": current_session.score,
		"total": current_session.total_attempts
	}

func end_session() -> Dictionary:
	if current_session == null:
		return {"success": false}
	
	var result = {
		"success": true,
		"total_score": current_session.score,
		"total_attempts": current_session.total_attempts,
		"percentage": float(current_session.score) / float(current_session.total_attempts) * 100.0 if current_session.total_attempts > 0 else 0.0,
		"words_practiced": current_session.words.size(),
		"session_progress": session_progress
	}
	
	current_session = null
	return result

func get_progress() -> Dictionary:
	if current_session == null:
		return {"words_mastered": 0, "total_words": vocabulary_data.size()}
	
	return {
		"current_score": current_session.score,
		"total_attempts": current_session.total_attempts,
		"activities_completed": current_session.current_index,
		"total_activities": current_session.activities.size(),
		"words_mastered": session_progress.size(),
		"total_words": vocabulary_data.size()
	}

func get_random_wrong_definitions(correct_word: Dictionary, count: int = 3) -> Array:
	var wrong_defs = []
	var available_words = vocabulary_data.filter(func(w): return w.word != correct_word.word)
	available_words.shuffle()
	
	for i in range(min(count, available_words.size())):
		wrong_defs.append(available_words[i].definition)
	
	return wrong_defs
