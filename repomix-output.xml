extends VBoxContainer

signal answer_submitted(is_correct: bool, answer: String)

var word_data: Dictionary

func setup(word: Dictionary):
	word_data = word
	var sentence = word.example.replace(word.word, "[color=yellow]___[/color]")
	sentence = sentence.replace(word.word.capitalize(), "[color=yellow]___[/color]")
	sentence = sentence.replace(word.word.to_upper(), "[color=yellow]___[/color]")
	$SentencePanel/Sentence.text = "[center]" + sentence + "[/center]"

func _on_submit_button_pressed():
	_on_submit($HBoxContainer/InputField.text)

func _on_submit(answer_text: String):
	var player_answer = answer_text.strip_edges().to_lower()
	var correct_word = word_data.word.to_lower()
	var is_correct = (player_answer == correct_word)
	answer_submitted.emit(is_correct, answer_text)
