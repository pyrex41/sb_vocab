<file path="vocab_replit/vocab_game/scripts/activities/synonym_antonym_activity.gd">
extends VBoxContainer

signal answer_submitted(is_correct: bool, answer: String)

var word_data: Dictionary
var choices: Array
var correct_index: int
var is_synonym_mode: bool

func setup(word: Dictionary):
	word_data = word
	is_synonym_mode = randi() % 2 == 0
	
	$Word.text = word.word
	
	if is_synonym_mode:
		$Title.text = "Choose a Synonym"
		setup_synonym_choices()
	else:
		$Title.text = "Choose an Antonym"
		setup_antonym_choices()

func setup_synonym_choices():
	if word_data.synonyms.size() == 0:
		return
	
	var correct_synonym = word_data.synonyms[randi() % word_data.synonyms.size()]
	var wrong_choices = []
	
	for other_word in MockBackend.vocabulary_data:
		if other_word.word != word_data.word and other_word.synonyms.size() > 0:
			wrong_choices.append(other_word.synonyms[0])
		if wrong_choices.size() >= 3:
			break
	
	wrong_choices.shuffle()
	choices = wrong_choices.slice(0, 3)
	correct_index = randi() % 4
	choices.insert(correct_index, correct_synonym)
	
	update_choice_buttons()

func setup_antonym_choices():
	if word_data.antonyms.size() == 0:
		return
	
	var correct_antonym = word_data.antonyms[randi() % word_data.antonyms.size()]
	var wrong_choices = []
	
	for other_word in MockBackend.vocabulary_data:
		if other_word.word != word_data.word and other_word.antonyms.size() > 0:
			wrong_choices.append(other_word.antonyms[0])
		if wrong_choices.size() >= 3:
			break
	
	wrong_choices.shuffle()
	choices = wrong_choices.slice(0, 3)
	correct_index = randi() % 4
	choices.insert(correct_index, correct_antonym)
	
	update_choice_buttons()

func update_choice_buttons():
	var choice_buttons = $ChoicesContainer.get_children()
	for i in range(min(4, choices.size())):
		choice_buttons[i].text = choices[i]

func _on_choice_pressed(index: int):
	var is_correct = (index == correct_index)
	answer_submitted.emit(is_correct, choices[index])
</file>

<file path="vocab_replit/vocab_game/scripts/activity_controller.gd">
extends Control

@onready var activity_container = $ActivityContainer
@onready var progress_bar = $ProgressBar
@onready var progress_label = $ProgressLabel
@onready var feedback_label = $FeedbackLabel

var current_activity_scene = null

func _ready():
	load_current_activity()
	update_progress_display()

func load_current_activity():
	for child in activity_container.get_children():
		child.queue_free()
	
	var activity = MockBackend.get_current_activity()
	if activity.is_empty():
		SessionManager.end_current_session()
		return
	
	var scene_path = ""
	match activity.type:
		"flashcard":
			scene_path = "res://scenes/activities/flashcard_activity.tscn"
		"multiple_choice":
			scene_path = "res://scenes/activities/multiple_choice_activity.tscn"
		"spelling":
			scene_path = "res://scenes/activities/spelling_activity.tscn"
		"fill_blank":
			scene_path = "res://scenes/activities/fill_blank_activity.tscn"
		"synonym_antonym":
			scene_path = "res://scenes/activities/synonym_antonym_activity.tscn"
	
	if ResourceLoader.exists(scene_path):
		var activity_scene = load(scene_path).instantiate()
		activity_container.add_child(activity_scene)
		activity_scene.setup(activity.word)
		
		if activity_scene.has_signal("answer_submitted"):
			activity_scene.answer_submitted.connect(_on_answer_submitted)
		current_activity_scene = activity_scene

func update_progress_display():
	var progress = MockBackend.get_progress()
	var total = progress.total_activities
	var completed = progress.activities_completed
	
	progress_bar.max_value = total
	progress_bar.value = completed
	progress_label.text = "Activity %d of %d" % [completed + 1, total]

func _on_answer_submitted(is_correct: bool, answer: String):
	show_feedback(is_correct)
	SessionManager.submit_answer(answer, is_correct)

func show_feedback(is_correct: bool):
	feedback_label.visible = true
	if is_correct:
		feedback_label.text = "Correct! ‚≠ê"
		feedback_label.add_theme_color_override("font_color", Color.GREEN)
	else:
		feedback_label.text = "Try again next time!"
		feedback_label.add_theme_color_override("font_color", Color.ORANGE)
