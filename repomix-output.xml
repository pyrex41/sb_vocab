extends Node

signal activity_completed(is_correct: bool)
signal session_ended(results: Dictionary)

var current_grade = 3
var last_session_results = {}

func start_new_session(grade: int = 3):
        current_grade = grade
        MockBackend.start_session(grade)
        get_tree().change_scene_to_file("res://scenes/activity_controller.tscn")

func submit_answer(answer: String, is_correct: bool):
        var result = MockBackend.submit_attempt(answer, is_correct)
        activity_completed.emit(is_correct)
        
        await get_tree().create_timer(1.5).timeout
        
        var next_activity = MockBackend.get_current_activity()
        if next_activity.is_empty():
                end_current_session()
        else:
                get_tree().reload_current_scene()

func end_current_session():
        var results = MockBackend.end_session()
        last_session_results = results
        session_ended.emit(results)
        get_tree().change_scene_to_file("res://scenes/results_screen.tscn")

func get_session_progress() -> Dictionary:
        return MockBackend.get_progress()
